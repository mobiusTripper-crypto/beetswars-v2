            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "axf-08-sdrk"
            }
          }
        ],
        "requestBody": {
          "description": "Contents of the new database entry",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Chartdata"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
