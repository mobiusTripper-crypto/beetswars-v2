{
  "openapi": "3.0.2",
  "info": {
    "title": "Beetswars Backend",
    "description": "Beetswars Backend API",
    "version": "1.1.2"
  },
  "servers": [
    {
      "url": "https://beetswars-backend.cyclic.app/API/v1"
    },
    {
      "url": "https://v2.beetswars.live/api/v1"
    }
  ],
  "paths": {
    "/healthcheck": {
      "get": {
        "tags": [
          "Check, if service is available"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/chartdata": {
      "get": {
        "tags": [
          "List of voting results"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/inline_response_200"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "example": "no data found"
                }
              }
            }
          }
        }
      }
    },
    "/chartdata/{round}": {
      "get": {
        "tags": [
          "List of voting results for specified round"
        ],
        "parameters": [
          {
            "name": "round",
            "in": "path",
            "description": "String with 2-figure round number",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "07"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/inline_response_200_1"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "example": "cannot serve request"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "No Object with given ID found"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Enter vote data for one round"
        ],
        "parameters": [
          {
            "name": "round",
            "in": "path",
