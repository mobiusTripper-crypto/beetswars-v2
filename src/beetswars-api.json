  "components": {
    "schemas": {
      "Chartdata": {
        "type": "object",
        "properties": {
          "round": {
            "type": "string",
            "example": "07"
          },
          "voteEnd": {
            "type": "integer",
            "format": "int32",
            "example": 1649001600
          },
          "totalVotes": {
            "type": "integer",
            "format": "int32",
            "example": 12345
          },
          "bribedVotes": {
            "type": "integer",
            "format": "int32",
            "example": 9345
          },
          "totalBribes": {
            "type": "integer",
            "format": "int32",
            "example": 123456
          },
          "totalVoter": {
            "type": "integer",
            "format": "int32",
            "example": 1234
          },
          "totalBriber": {
            "type": "integer",
            "format": "int32",
            "example": 12
          },
          "priceBeets": {
            "type": "number",
            "format": "double",
            "example": 0.7449121150496086
          },
          "priceFbeets": {
            "type": "number",
            "format": "double",
            "example": 0.74271
          }
        }
      },
      "Bribefile": {
        "type": "object",
        "properties": {
          "version": {
            "type": "string",
            "example": "7.5.1"
          },
          "snapshot": {
            "type": "string",
            "example": "0xa3478bd5cbe2b422b94dfcccae5277703ba08f923b10ba710062eef08ec672d5"
          },
          "description": {
            "type": "string",
            "example": "Farming Incentive Gauge Vote (round 7)"
          },
          "round": {
            "type": "integer",
            "example": 7
          },
          "voteStart": {
            "type": "integer",
            "format": "int32",
            "example": 1649001600
          },
          "voteEnd": {
            "type": "integer",
            "format": "int32",
            "example": 1649001600
          },
          "snapshotDateTime": {
            "type": "integer",
            "format": "int32",
            "example": 1649001600
          },
          "tokendata": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tokendata"
            }
          },
          "bribedata": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Bribedata"
            }
          }
        }
      },
      "Tokendata": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "example": "BEETS"
          },
          "tokenaddress": {
            "type": "string",
            "example": "0xf24bcf4d1e507740041c9cfd2dddb29585adce1e"
          },
          "coingeckoid": {
            "type": "string",
            "example": "beethoven-x"
          },
          "bptpoolid": {
            "type": "string",
            "example": "0xf3a602d30dcb723a74a0198313a7551feaca7dac00010000000000000000005f"
          },
          "isbpt": {
            "type": "boolean",
            "example": false
          },
          "lastprice": {
            "type": "number",
            "format": "float",
            "example": 1.034
          }
        }
      },
      "Bribedata": {
        "type": "object",
        "properties": {
          "voteindex": {
            "type": "integer",
            "description": "Number of pool zero-based",
            "example": 0
          },
          "poolname": {
            "type": "string",
            "example": "Fantom of the Opera (FTM-USDC)"
          },
          "poolurl": {
            "type": "string",
            "format": "url",
            "example": "https://beets.fi/pool/0xcdf68a4d525ba2e90fe959c74330430a5a6b8226000200000000000000000008"
          },
          "rewarddescription": {
            "type": "string",
            "example": "BEETS Reward 60,563.51 BEETS"
          },
          "assumption": {
            "type": "string",
            "example": "Raffle for 3 FNFTs not included in calculation"
          },
          "percentagethreshold": {
            "type": "number",
            "format": "float",
            "example": 17.5
          },
          "rewardcap": {
            "type": "number",
            "format": "float",
            "example": 9000
          },
          "additionalrewards": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Additionalrewards"
            }
          },
          "reward": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Reward"
            }
          },
          "payoutthreshold": {
            "type": "number",
            "format": "float",
            "example": 4.25
          }
        }
      },
      "Reward": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "description": "reward-type",
            "example": "percent",
            "enum": [
              "fixed",
              "percent",
              "pervote"
            ]
          },
          "token": {
            "type": "string",
            "example": "BEETS"
          },
          "amount": {
            "type": "number",
            "format": "float",
            "example": 1234.56
          },
